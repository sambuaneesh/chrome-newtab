{
  "manifest_version": 2,
  "name": "Deadline Tracker New Tab",
  "version": "1.0",
  "description": "Replace new tab with a Deadline Tracker app",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "deadline-tracker@example.com"
    }
  }
}
